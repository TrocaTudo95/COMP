
module array1
{


  function print_array(N)
  {

   a=[N];


   i=0;
   while(i < N)
   {
   	a[i] = i;
	i=i+1;
	}

	i=0;
	while(i < N) {
		ai=a[i];
		io.print("a: ",ai);
		i=i+1;
	}
	}

	function main()
	{
	 print_array(a); // the same as array1.print_array(10);
   a=10;
	}

}
